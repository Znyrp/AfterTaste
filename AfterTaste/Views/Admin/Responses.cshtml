@model IEnumerable<ContactUs>;

<!-- ======= Contact Section ======= -->
<section id="recipe" class="recipe">
    <div class="container">

        <div class="section-title mt-5">
            <h2> Responses</h2>
@*             <p class="pb-2">
                Ut possimus qui ut temporibus culpa velit eveniet modi omnis est adipisci expedita at voluptas
                atque vitae
                autem.
            </p> *@
        </div>

        <table class="table table-hover">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">Name</th>
                    <th scope="col">Email</th>
                    <th scope="col">Subject</th>
                    <th scope="col">Message</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var ContactUs in Model)
                {
                    <tr>
                        <th scope="row">@ContactUs.contactId</th>
                        <td>@ContactUs.fullName</td>
                        <td>@ContactUs.email</td>
                        <td>@ContactUs.subject</td>
                        <td>@ContactUs.message</td>
                    </tr>
                } 
            </tbody>
        </table>
    </div>
</section>